<%= simple_form_for @locker do |f| %>
	<p>
		<%= f.label :ref %><br>
		<%= f.text_field :ref %>
	</p>

	<p>
		<%= f.label :floor %><br>
		<%= f.text_field :floor %>
	</p>

	<p>
		<%= f.label :location %><br>
		<%= f.text_field :location %>
	</p>

	<p>
		<%= f.label :size %><br>
		<%= f.text_field :size %>
	</p>

	<%= f.button :submit %>
<% end %>